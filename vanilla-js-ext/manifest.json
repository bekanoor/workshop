{
  "name": "My Extension with JS",
  "description": "Simple counter",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "./src/images/js-16x16.png",
    "32": "./src/images/js-32x32.png",
    "48": "./src/images/js-48x48.png",
    "128": "./src/images/js-128x128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "./src/popup.html",
    "default_icon": {
      "16": "./src/images/js-16x16.png",
      "32": "./src/images/js-32x32.png",
      "48": "./src/images/js-48x48.png",
      "128": "./src/images/js-128x128.png"
    }
  }
}